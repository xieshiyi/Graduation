extends layout

block content

  nav(class="navbar navbar-inverse navbar-fixed-top")
     .container-fluid
      .navbar-header 
        button(
          class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar"
        )
          span(class="sr-only") Toggle navigation       
          span(class="icon-bar")
          span(class="icon-bar")
          span(class="icon-bar")
        a(class="navbar-brand" href="#") 物位计监控系统
      div(id='navbar' class='navbar-collapse collapse') 
        ul(class="nav navbar-nav navbar-right") 
          li
            a(href="#") 帮助

  .container-fluid
    .row
      div(class='col-sm-3 col-md-2 sidebar')
        .fan
          ul(class='nav nav-sidebar')
            li(class='nav_title one active')
              a(href='#') 物位计
                span(class='sr-only') (current)
            li(class="nav_title two")
              a(href="#") 超声波
            li(class="nav_title three")
              a(href="#") 电容式
            li(class="nav_title four")
              a(href="#") 微波雷达
            li(class="nav_title five")
              a(href="#") 静压式
              
      //版块一
      div(class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main pageOne")
        h1(class="page-header") 物位计
        div(class='row placeholders')
          div(class='col-xs-6 col-sm-3 placeholder')
            img(src="/images/1.png" width="200" height="200" class="img-responsive imgOne" alt="Generic placeholder thumbnail")
          div(class='col-xs-6 col-sm-3 placeholder')
            img(src="/images/2.png" width="200" height="200" class="img-responsive imgTwo" alt="Generic placeholder thumbnail")
          div(class='col-xs-6 col-sm-3 placeholder')
            img(src="/images/3.png" width="200" height="200" class="img-responsive imgThree" alt="Generic placeholder thumbnail")
          div(class='col-xs-6 col-sm-3 placeholder')
            img(src="/images/4.png" width="200" height="200" class="img-responsive imgFour" alt="Generic placeholder thumbnail")
        h2(class='sub-header') 报警提示邮箱 
        br
        h2(class="text-muted" style="float: left;") 邮箱账号：
        input(type="text" class="form-control email" placeholder="请输入正确邮箱格式")
        button(type="submit" class="btn btn-primary" style='margin-left:4px;') 确定
      //版块二
      div(class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main pageTwo")
        h1(class="page-header") 超声波物位计
        .table-responsive
          div(class='col-md-6 col-xs-12 col-sm-12 placeholder chart' id='')
            ul(class="nav nav-tabs" role="tablist")
                li(role="presentation" class="nav_tabs_titleOne time_dataOne active")
                  a(href="#") 时间段监控 
                li(role="presentation" class='nav_tabs_titleOne alarm_dataOne')
                  a(href="#") 报警详情
            div(class='time_data_page time_data_pageOne')           
              mixin choose 
                ul(class="navbar-nav time_choose")
                  li(class="dropdown")
                    a(href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false") 3个小时内
                      span(class="caret")
                    ul(class="dropdown-menu")
                      li
                        a(href='#' class='dropdown-menu-one') 12个小时内
                      li
                        a(href='#' class='dropdown-menu-two') 一天内
              +choose
              div(class='placeholder chart-data' id='data-line-chart')
            div(class='alarm_data_page alarm_data_pageOne')
              .placeholder
                +choose
                table(class="table table-striped")
                  thead
                    tr
                      th 
                      th 日期
                      th 时间
                      th 高度                                    
                  tbody
                    tr
                      td -
                      td -
                      td -
                      td -                  
          
          div(class='col-md-6 col-xs-12 col-sm-12 placeholder chart' id='line-chart')

      //版块三
      div(class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main pageThree")
